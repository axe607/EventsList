                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUpdate"
                contract="EventService.IUpdate" name="BasicHttpBinding_IUpdate" />
    </client>
  </system.serviceModel>
  
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
